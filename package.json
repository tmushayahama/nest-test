{
  "name": "panther-keyword-api",
  "private": true,
  "version": "1.0.0",
  "description": "Panther Keyword Api",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug-watch": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.8.0",
    "@hapi/joi": "17.1.1",
    "@nestjs/common": "7.3.2",
    "@nestjs/core": "7.3.2",
    "@nestjs/elasticsearch": "^7.1.0",
    "@nestjs/jwt": "7.1.0",
    "@nestjs/passport": "7.1.0",
    "@nestjs/platform-express": "7.3.2",
    "@nestjs/swagger": "4.5.12",
    "@types/elasticsearch": "^5.0.36",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "1.0.1",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "dotenv-safe": "^8.2.0",
    "elasticsearch": "^16.7.1",
    "express-flash": "0.0.2",
    "express-rate-limit": "5.1.3",
    "express-validator": "^6.6.0",
    "gravatar": "1.8.0",
    "helmet": "3.23.3",
    "lodash": "^4.17.19",
    "nest-access-control": "2.0.2",
    "nestjs-command": "^1.5.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "3.0.2",
    "rxjs": "6.6.0",
    "swagger-ui-express": "4.1.4",
    "validator": "^13.1.1",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.11",
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "7.3.2",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.7",
    "@types/gravatar": "1.8.1",
    "@types/hapi__joi": "17.1.4",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.23",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "2.0.10",
    "@types/validator": "^13.1.0",
    "@types/yargs": "^15.0.8",
    "chai": "4.2.0",
    "jest": "26.1.0",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "26.1.2",
    "ts-loader": "8.0.1",
    "ts-node": "8.10.2",
    "tsc-watch": "4.2.9",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.2",
    "typescript": "3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
